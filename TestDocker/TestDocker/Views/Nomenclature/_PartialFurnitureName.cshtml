@model TestDocker.ViewsModels.EditNomenclatureViewModel

<table class="table-sm table-striped table-responsive-md btn-table">
    <thead>
        <tr>
            <th colspan="2" class="text-center">FurnitureNames</th>
        </tr>
        <tr>
            <td>Brand.Collection.FurnitureName</td>
        </tr>
    </thead>
    <tbody>
        @foreach (FurnitureNameViewModel furnitureNameViewModel in Model.FurnitureNameViewModels)
        {
            <tr>
                <td scope="row">@furnitureNameViewModel.BrandName.@furnitureNameViewModel.CollectionName.@furnitureNameViewModel.Name
                </td>
                <td>
                    <a class="btn btn-red btn-sm m-0"
                       asp-controller="Nomenclature"
                       asp-action="DeleteFurnitureName"
                       asp-route-id="@furnitureNameViewModel.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
